{
  "$schema": "https://json.schemaecommerce.org/launchsettings.json",
  "profiles": {
    "ApiGateway.Https": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "hotReloadProfile": "aspnetcore",
      "launchBrowser": true,
      "launchUrl": "",
      "applicationUrl": "https://localhost:3010;http://localhost:3000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "ApiGateway.Http": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "hotReloadProfile": "aspnetcore",
      "launchBrowser": true,
      "launchUrl": "",
      "applicationUrl": "http://localhost:3000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "ApiGateway.Watch": {
      "commandName": "Project",
      "executablePath": "dotnet",
      "workingDirectory": "$(ProjectDir)",
      "hotReloadEnabled": true,
      "hotReloadProfile": "aspnetcore",
      "commandLineArgs": "watch -lp ApiGateway.Http",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
	  "ApiGateway.LiveRecompilation": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "",
      "applicationUrl": "http://localhost:3000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}
